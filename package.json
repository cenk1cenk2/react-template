{
  "name": "@cenk1cenk2/react-template",
  "private": true,
  "description": "Scaffolding for react/next.js templates.",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target=serve --parallel --maxParallel=1000 --all",
    "start:one": "nx serve",
    "build": "nx run-many --target=build --parallel --maxParallel=1000 --all",
    "build:one": "nx build",
    "build:nocache": "yarn build --skip-nx-cache",
    "test": "nx run-many --target=test --parallel --maxParallel=1000 --all",
    "test:one": "nx test",
    "test:e2e": "nx run-many --target=e2e --parallel --maxParallel=1000 --all",
    "lint": "nx format:write --all && nx workspace-lint && yarn lint:check --fix",
    "lint:check": "nx run-many --parallel --maxParallel=5 --target=lint --all --",
    "lint:one": "nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx format:write --base=$(git branch --show-current) && nx affected:lint --parallel --maxParallel=5 --fix --base=$(git branch --show-current) --skip-nx-cache --",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "clean": "rimraf dist/ .cache/ node_modules/ yarn.lock yarn-error.log package-lock.json",
    "scripts:link": "./scripts/link-packages.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ]
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-is": "^17.0.2",
    "recoil": "^0.3.1",
    "styled-components": "^5.3.0"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "clsx": "^1.1.1",
    "config": "^3.3.6",
    "document-register-element": "^1.14.10",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.3.1",
    "styled-components": "^5.3.0",
    "use-debounce": "^7.0.0",
    "@material-ui/core": "^4.12.1"
  },
  "devDependencies": {
    "roboto-fontface": "^0.10.0",
    "fontsource-frank-ruhl-libre": "^4.0.0",
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@cenk1cenk2/eslint-config": "^0.4.42",
    "@nrwl/cli": "^12.5.8",
    "@nrwl/eslint-plugin-nx": "^12.5.8",
    "@nrwl/jest": "^12.5.8",
    "@nrwl/node": "^12.5.8",
    "@nrwl/react": "^12.5.8",
    "@nrwl/web": "^12.5.8",
    "@nrwl/workspace": "^12.5.8",
    "@testing-library/react": "12.0.0",
    "@types/config": "^0.0.39",
    "@types/jest": "26.0.24",
    "@types/node": "^14.17.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-is": "17.0.2",
    "@types/react-router-dom": "5.1.8",
    "@types/styled-components": "5.1.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-jest": "27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "babel-preset-minify": "^0.5.1",
    "dotenv": "10.0.0",
    "error-overlay-webpack-plugin": "^0.4.2",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.1",
    "jest": "27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "~10.1.0",
    "typescript": "~4.3.5"
  }
}
